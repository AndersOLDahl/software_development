<h1 id="helmuth-title">Helmuth Microsite</h1>

<% if user_signed_in? %>
    <div id="helmuth-login"><button>
    <%= link_to('Logout', destroy_user_session_path, method: :delete) %>
    </button></div>
<% else %>
    <div id="helmuth-login"><button>
    <%= link_to('Login', new_user_session_path) %>
    </button></div>
<% end %>

<div id="helmuth-map" style='width: 800px;'>
    <div id="map" data-microsites="<%= @hash.to_json %>" style='width: 100%; height: 400px;'>
    </div>
</div>
